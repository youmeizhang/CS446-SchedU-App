    android:layout_height="match_parent"
    android:stretchColumns="6" >
    <TableRow>
        <TextView
            android:text=""/>
        <TextView
            android:text="Monday"
            android:padding="3dip"/>

        <TextView
            android:padding="3dip"
            android:text="Tuesday" />
        <TextView
            android:text="Wednesday"
            android:padding="3dip"/>
        <TextView
            android:text="Thursday"
            android:padding="3dip"/>
        <TextView
            android:text="Friday"
            android:padding="3dip" />
